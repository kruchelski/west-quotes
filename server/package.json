{
  "name": "west-quotes",
  "version": "1.0.0",
  "description": "This is an application to consume the kanye.rest API and Unsplash API",
  "main": "server.js",
  "scripts": {
    "postinstall": "node -e \"require('fs').existsSync('.env') || require('fs').copyFileSync('.env.example', '.env')\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server.js",
    "devStart": "nodemon server.js",
    "debug": "nodemon --inspect server.js",
    "migrations": "sequelize db:migrate",
    "seeders": "sequelize db:seed:all",
    "migrateAndStart": "echo \"Running migrations\" && sequelize db:migrate && nodemon server.js",
    "fullStart": "echo \"Running migrations\" && sequelize db:migrate && echo \"Running seeds\" && sequelize db:seed:all && nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kruchelski/west-quotes.git"
  },
  "keywords": [
    "kanye",
    "rest",
    "quotes"
  ],
  "author": "Cassiano Kruchelski",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kruchelski/west-quotes/issues"
  },
  "homepage": "https://github.com/kruchelski/west-quotes#readme",
  "dependencies": {
    "axios": "~0.21.1",
    "bcrypt": "^5.0.1",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "jsonwebtoken": "^8.5.1",
    "pg": "^8.5.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@lacussoft/eslint-config": "^1.0.1",
    "dotenv": "^8.2.0",
    "eslint": "^7.22.0",
    "nodemon": "^2.0.6",
    "sequelize-cli": "^6.2.0",
    "sucrase": "^3.17.1"
  }
}
